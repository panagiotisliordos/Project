    <h1>All the routes</h1>
<div class="main-page-routes">
    {{#each runRouteCopy}}
    <div class="all-the-cards">
            <div class="card">
           <img src="{{image}}" class="card-img-top" alt="..." width="50px" >
        <div class="card-body">
            <h3 class="card-title">{{title}}</h3>
            <h6>Date: {{date}}</h6>
            <h6>Time:{{time}}</h6>
            <h6>Starting point:{{startingPoint}}</h6>
            <h6>Distance:{{distance}}</h6>
            <h6>Duration:{{duration}}</h6>
            <h6>Location:{{location}}</h6>
            <h6>Pace:{{pace}}</h6>
            <h6>Difficulty level:{{difficultyLevel}}</h6>
            <h6>Organizer:{{organizer}}</h6>
            <a href="/attend/{{_id}}" class="btn btn-primary">Attend</a>
        </div>
            </div>

    </div>
{{/each}}
</div>
